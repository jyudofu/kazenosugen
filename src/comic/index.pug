extend /_includes/_layout
append variables
  //- Required
  - var pageTitle= "COMIC";
  - var pageDescription= site.description;
  - var pageKeywords= site.keywords;
  //- Optional
  - var pageOgpTitle= "COMIC";
  - var pageOgpImage= site.ogpImage
  - var pageLang= "ja";
  - var pageOgpType= "website";
  //- Not modified
  - var pageUrl= relativePath;
  - var nowPage= "pageTop";
  - var pageTop="COMIC"

block content
    .main
    .selectGenre
        #js_touhou.selectGenrebox.touhou.firstSelect
            p 東方Project
        #js_symphogear.selectGenrebox.symphogear.firstSelect
            p 戦姫絶唱シンフォギア
        #touhou.hide
            h2 東方天王禄シリーズ
            nav: ul.mainLi_ul.clearfix
                li: .mangainner.tenourokuTenKitou.fade: a(href="./tenourokuTenKitou/").manga
                    p.mangaTitle 東方天王禄 天<br>─祈祷編─
                li: .mangainner.tenourokuU.fade: a(href="./tenourokuU/").manga
                    p.mangaTitle 東方天王禄 有
            h2 霊夢×天子本
            nav: ul.mainLi_ul.clearfix
                li: .mangainner.hinekuremon.fade: a(href="./hinekuremon/").manga
                    p.mangaTitle ひねくれもん
                li: .mangainner.kimikimi.fade: a(href="./kimikimi/").manga
                    p.mangaTitle 君に、君と
            h2 東方狂天狗シリーズ
            nav: ul.mainLi_ul.clearfix
                li: .mangainner.kyoutengu0.fade: a(href="./kyoutengu0/").manga
                    p.mangaTitle 東方狂天狗 零
                li: .mangainner.kyoutengu1.fade: a(href="./kyoutengu1/").manga
                    p.mangaTitle 東方狂天狗 壱
                li: .mangainner.kyoutengu2.fade: a(href="./kyoutengu2/").manga
                    p.mangaTitle 東方狂天狗 弐
                li: .mangainner.kyoutengu3.fade: a(href="./kyoutengu3/").manga
                    p.mangaTitle 東方狂天狗 参
                li: .mangainner.kyoutengu4.fade: a(href="./kyoutengu4/").manga
                    p.mangaTitle 東方狂天狗 肆
                li: .mangainner.kyoutengu5.fade: a(href="./kyoutengu5/").manga
                    p.mangaTitle 東方狂天狗 伍
                li: .mangainner.kyoutenguLast.fade: a(href="./kyoutenguLast/").manga
                    p.mangaTitle 東方狂天狗 終
            h2 その他
            nav: ul.mainLi_ul.clearfix
                li: .mangainner.mahounoresipi.fade: a(href="./mahounoresipi/").manga
                    p.mangaTitle 魔法のレシピ
        #symphogear.hide
            h2 ストーリー漫画
            nav: ul.mainLi_ul.clearfix
                li: .mangainner.rainyMood.fade: a(href="./rainyMood/").manga
                    p.mangaTitle RAINY MOOD
            h2 その他
            nav: ul.mainLi_ul.clearfix
                li: .mangainner.kusoManga001.fade: a(href="./kusoManga001/").manga
                    p.mangaTitle クソ漫画GX<br>━ つばクリポッキーゲーム編 ━
    #js_backToTop
        a(href="#").back_to_top: .arrow
